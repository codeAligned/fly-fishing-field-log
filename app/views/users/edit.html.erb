<h1>Edit Your Information</h1>
<%= form_for @user do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :age %>
    <%= f.text_field :age %>
  </p>
  <p>
    <%= f.label :name, 'State' %>
    <%= f.collection_select(:state_id, State.all, :id, :name) %>
  </p>
  <%= f.submit %>
<% end %>
