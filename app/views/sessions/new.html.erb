
<label>Sign In</label>
 <%= form_tag '/sessions' do %>
  <%= label_tag :name %>
  <%= text_field_tag 'user[name]' %><br>
  <%= label_tag :password %>
  <%= password_field_tag 'user[password]' %><br>
  <%= submit_tag "Log In"%>
<% end %>
<%= flash[:message]  %>

<br>
<br>
<label>Sign Up!</label>
<p><%= button_to "New User Sign Up", new_user_path %></p>
